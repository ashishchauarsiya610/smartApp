<script>
if ( location.href.indexOf('sales-and-promos')==-1 && location.href.indexOf('joinNow.do')==-1) { // Use `false` to disable popup
    document.onreadystatechange = function() {
        if (document.readyState === 'complete') {
            if (sessionStorage["PopupShown"] != 'yes') {
                if ($.cookie('popup') != 'seen') {
                    // Get the modal
                    var modal = document.getElementById('sales-modal');
                    if (modal) {
                        modal.style.display = "block";

                        var elems = document.querySelectorAll(".hidden-sale");
                        [].forEach.call(elems, function(el) {
                            el.className = el.className.replace(/\bhidden-sale\b/, "");
                        });

                        $.cookie('popup', 'seen', {
                            expires: 1,
                            path: '/'
                        });
                        sessionStorage["PopupShown"] = 'yes';

                        // Get the <span> element that closes the modal
                        var span = document.getElementsByClassName("close-sale")[0];

                        // When the user clicks on <span> (x), close the modal
                        span.onclick = function() {
                            modal.style.display = "none";
                        }

                        var modalContent = document.getElementsByClassName("modal-content-sale");

                        // When the user clicks anywhere outside of the modal, close it
                        window.onclick = function(event) {
                            if (event.target == modal) {
                                modal.style.display = "none";
                            }

                            if (event.target == modalContent[0]) {
                                window.location.href = "/cyber-monday-offer/";
                            }
                        }   
                    }
                }
            }
        }
    };
}
</script>

