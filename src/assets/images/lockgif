<script>
document.addEventListener('DOMContentLoaded', function() {
    var planName = getUrlVars()["planName"];
    if (planName === "kids")
        document.GenericForm.action = '/faith/freeTrial.do?planName=kids';
}, false);

function getUrlVars() {
    var vars = {};
    window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
        vars[key] = value;
    });
    return vars;
}
</script>


