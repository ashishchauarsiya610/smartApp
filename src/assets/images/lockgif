<script>
  document.addEventListener('DOMContentLoaded', function(){ 
    var planName = getUrlVars()["planName"];
    var kidsHeader = document.getElementById("kidsHeader");

    if(planName != null)
    {
      if(planName === "kids" ){
        kidsHeader.style.display = "block";
      }
      else{
        kidsHeader.style.display = "none";
      }
    }else{
      kidsHeader.style.display = "none";
    }
}, false);

function getUrlVars() {
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
        vars[key] = value;
    });
    return vars;
}
</script></div>
