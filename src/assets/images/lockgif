<script>

/**
 * detect IE returns version of IE or false, if browser is
 * not Internet Explorer
 */
function detectIE() {
  var ua = window.navigator.userAgent;

  var msie = ua.indexOf('MSIE ');
  if (msie > 0) {
    // IE 10 or older => return version number
    return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
  }

  var trident = ua.indexOf('Trident/');
  if (trident > 0) {
    // IE 11 => return version number
    var rv = ua.indexOf('rv:');
    return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
  }

  var edge = ua.indexOf('Edge/');
  if (edge > 0) {
    // Edge (IE 12+) => return version number
    return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
  }

  // other browser
  return 100;
}

	$( document )
			.ready(
					function()
					{
						$( '#processing_loader' ).hide();

						if(detectIE() <= 9){
							$( "#braintreeIncompatiableModal" ).modal(
									{
										keyboard : false,
										show : true,
										backdrop : 'static'
									} );
									$( '#credit_loader' ).hide();
									Raven.captureException( new Error("IE Version is lower or equal than 9"));

							return;
						}

						try
						{
							//Make sure we are in testing mode.
							if( getQueryString( "bill" ) != "test" )
							{

								//Hide all credit card erros from displaying to the user.
								hideAllErrors();

								braintree.client
										.create(
												{
													authorization : "production_gq3qvh6n_52x225t7snbympgc"
				}, function( err, clientInstance )
				{
					if( err )
					{
						//Can be found in footer.jsp
						$( "#braintreeIncompatiableModal" ).modal(
						{
							keyboard : false,
							show : true,
							backdrop : 'static'
						} );
						$( '#credit_loader' ).hide();
						Raven.captureException( new Error( "Error creating billing form occured!" ) );
						Raven.captureException( new Error( err ) );
						return;
					}
					braintree.hostedFields.create(
					{
						client : clientInstance,
						styles :
						{
							'select, input' :
							{
								'font-size' : '15px',
								'font-family' : '"Open Sans", sans-serif',
								'color' : '#727070',
								'padding' : '0 0 0 18px'
							},
							'input:focus::placeholder' :
							{
								'opacity' : '0'
							}
						},
						fields :
						{
							number :
							{
								selector : '#ccnum',
								placeholder : 'Credit Card Number'
							},
							cvv :
							{
								selector : '#ccCVV',
								placeholder : 'CVV'
							},
							expirationMonth :
							{
								selector : '#month',
								placeholder : 'Month',
								select :
								{
									options :
									[ '01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12' ]
								}
							},
							expirationYear :
							{
								selector : '#year',
								placeholder : 'Year',
								select : true
							},
							postalCode :
							{
								selector : '#zip',
								placeholder : 'Postal Code'
							}
						}
					}, function( err, hostedFieldsInstance )
					{
						if( err )
						{
							//Can be found in footer.jsp
							$( "#braintreeIncompatiableModal" ).modal(
							{
								keyboard : false,
								show : true,
								backdrop : 'static'
							} );
							$( '#credit_loader' ).hide();
							Raven.captureException( new Error( "Error creating billing form occured!" ) );
							Raven.captureException( new Error( err ) );
							return;
						}

						$( '#credit_loader' ).hide();

						//hostedFieldsInstance.on( 'validityChange', function( event )
						//{
						// Check if all fields are valid, then show submit button
						//	var formValid = Object.keys( event.fields ).every( function( key )
						//	{
						//		return event.fields[ key ].isValid;
						//	} );
						//} );

						hostedFieldsInstance.on( 'empty', function( event )
						{
							$( 'header' ).removeClass( 'header-slide' );
							$( '#card-image' ).removeClass();
						} );

						hostedFieldsInstance.on( 'cardTypeChange', function( event )
						{
							// Change card bg depending on card type
							if( event.cards.length === 1 )
							{
								$( '#card-image' ).removeClass().addClass( event.cards[ 0 ].type );
								$( 'header' ).addClass( 'header-slide' );

								// Change the CVV length for AmericanExpress cards
								if( event.cards[ 0 ].code.size === 4 )
								{
									hostedFieldsInstance.setAttribute(
									{
										field : 'cvv',
										attribute : 'placeholder',
										value : 'CVV'
									} );
								}
							}
							else
							{
								hostedFieldsInstance.setAttribute(
								{
									field : 'cvv',
									attribute : 'placeholder',
									value : 'CVV'
								} );
							}
						} );

						$( '.btn.btn-success.btn-reg-type' ).click( function( event )
						{
                            event.preventDefault();
                            if (! validationCCPrompts())
                                return;
							if (! ccFieldsAreInvalid(hostedFieldsInstance._state.fields)) {
								hostedFieldsInstance.tokenize( function( tokenizeErr, payload )
								{
									if( tokenizeErr )
									{
										console.error( tokenizeErr );

										switch( tokenizeErr.code )
										{
											case 'HOSTED_FIELDS_FIELDS_EMPTY':
												//All fields are blank so lets show all the errors.
												var allFields =
												[ "number", "cvv", "expirationMonth", "expirationYear", "postalCode" ];

												//showErrors( allFields );
												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "All Empty Credit Card Fields";
												$( '#submitPayment' ).click();
												break;
											case 'HOSTED_FIELDS_FIELDS_INVALID':
												if( tokenizeErr.details.invalidFieldKeys != null )
												{
													//Goes trough the array and will show the errors found.
													//showErrors( tokenizeErr.details.invalidFieldKeys );
													//Send payment merchant response to server
													document.getElementById( "payment-response" ).value = "Invalid Credit Card Fields";
													$( '#submitPayment' ).click();
												}
												break;
											case 'HOSTED_FIELDS_TOKENIZATION_FAIL_ON_DUPLICATE':
												$( "#braintreeIncompatiableModal" ).modal(
												{
													keyboard : false,
													show : true,
													backdrop : 'static'
												} );
												$( '#credit_loader' ).hide();
												Raven.captureException( new Error( "Duplicate Transaction" ) );
												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "Duplicate Transaction";
												$( '#submitPayment' ).click();
												break;
											case 'HOSTED_FIELDS_TOKENIZATION_CVV_VERIFICATION_FAILED':
												showErrors( 'cvv' );
												$( "#braintreeIncompatiableModal" ).modal(
												{
													keyboard : false,
													show : true,
													backdrop : 'static'
												} );
												$( '#credit_loader' ).hide();
												Raven.captureException( new Error( "CVV verification failed" ) );

												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "CVV Verification Failed";
												$( '#submitPayment' ).click();
												break;
											case 'HOSTED_FIELDS_FAILED_TOKENIZATION':
												$( "#braintreeIncompatiableModal" ).modal(
												{
													keyboard : false,
													show : true,
													backdrop : 'static'
												} );
												$( '#credit_loader' ).hide();

												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "Could not get Braintree Nonce";
												$( '#submitPayment' ).click();
												break;
											case 'HOSTED_FIELDS_TOKENIZATION_NETWORK_ERROR':
												$( "#braintreeIncompatiableModal" ).modal(
												{
													keyboard : false,
													show : true,
													backdrop : 'static'
												} );
												$( '#credit_loader' ).hide();
												Raven.captureException( new Error( "Tokenization error" ) );

												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "Network Error";
												$( '#submitPayment' ).click();
												break;
											default:
												$( "#braintreeIncompatiableModal" ).modal(
												{
													keyboard : false,
													show : true,
													backdrop : 'static'
												} );
												$( '#credit_loader' ).hide();
												Raven.captureException( new Error( "Generic error!" ) );

												//Send payment merchant response to server
												document.getElementById( "payment-response" ).value = "Generic Error";
												$( '#submitPayment' ).click();
										}
									}
									else
									{
										$( '#processing_loader' ).show();

										setTimeout( function()
										{
											document.getElementById( 'processing-icon' ).setAttribute( 'src', 'https://images.sharefaith.com/images/img/3/central/images/icons/processing-solid.gif' )
										}, 1000 )

										//Hack: TODO Find a better way to do this.
										document.getElementById( "payment_method_nonce" ).value = payload.nonce;
										document.getElementById( "zip" ).value = "999999";
										document.getElementById( "payment-response" ).value = "success";
										$( '#submitPayment' ).click();
									}
								} );
							}
						} );
					} );
				} );
			}
		}
		catch( error )
		{
			//Can be found in footer.jsp
			$( "#braintreeIncompatiableModal" ).modal(
			{
				keyboard : false,
				show : true,
				backdrop : 'static'
			} );
			$( '#credit_loader' ).hide();
			Raven.captureException( error );
		}
	} );

	function showErrors( errorArray )
	{
		//Make sure we are in testing mode.
		if( getQueryString( "bill" ) != "test" )
		{
			var allFields = errorArray;

			//Hide the error messages so that we only show the ones that apply.
			hideAllErrors()

			//Show the apropriate errors.
			for( var i = 0; i < errorArray.length; i++ )
			{
				var errorElement = "";

				if( allFields[ i ] == "expirationMonth" || allFields[ i ] == "expirationYear" )
				{
					if( document.getElementById( "cvv" ).style.display == 'block' )
					{
						document.getElementById( "expirationDate" ).style.display == 'none';
					}
					else
					{
						errorElement = document.getElementById( "expirationDate" );
						document.getElementById( "cvv" ).style.display == 'none';
						errorElement.style.display = 'block';
					}
				}
				else
				{
					if( document.getElementById( "expirationDate" ).style.display == 'block' && allFields[ i ] == "cvv" )
					{
						document.getElementById( "cvv" ).style.display == 'none';
					}
					else
					{
						errorElement = document.getElementById( allFields[ i ] + "" );
						errorElement.style.display = 'block';

						if (allFields[ i ] === 'number') {
							$('#card-image').css('top', '40px');
						}
					}
				}
			}
		}
	}

	/**
	 * Hide the error messages for credit card information
	 */
	function hideAllErrors()
	{
		//Make sure we are in testing mode.
		if( getQueryString( "bill" ) != "test" )
		{
			var allFields =
			[ "number", "cvv", "expirationMonth", "expirationYear", "postalCode", "firstName", "lastName", "country" ];

			//We need to hide all credit card errors.
			for( var i = 0; i < allFields.length; i++ )
			{
				var errorElement = "";

				if( allFields[ i ] == "expirationMonth" || allFields[ i ] + "" == "expirationYear" )
				{
					errorElement = document.getElementById( "expirationDate" );
					errorElement.style.display = 'none';
				}
				else
				{
					errorElement = document.getElementById( allFields[ i ] + "" );
					errorElement.style.display = 'none';
				}
			}
			$('#card-image').css('top', '9px');
		}
	}

	/**
	 * Get the value of a querystring
	 * @param  {String} field The field to get the value of
	 * @param  {String} url   The URL to get the value from (optional)
	 * @return {String}       The field value
	 */
	function getQueryString( field, url )
	{
		var href = url ? url : window.location.href;
		var reg = new RegExp( '[?&]' + field + '=([^&#]*)', 'i' );
		var string = reg.exec( href );
		return string ? string[ 1 ] : null;
	}

	/**
	 * Check if fields are empty or invalid in form and trigger error messages if so
	 * @param {Object} hostedFieldState An object with info from Brainstree on the CC hosted fields
	 * @return {Boolean}                True or False as to whether or not fields are empty/invalid
	 */
	function ccFieldsAreInvalid(hostedFieldState) {
		var nativeFields = ['firstName', 'lastName', 'country'];
		var invalidFields = {
			native: [],
			hosted: []
		};
		// Check if the native name fields are empty
		$.each(nativeFields, function(index, name) {
			var value = $('[name="value(' + name + ')"]').val();
			if(isConsideredEmpty(value, ['Card Holder First Name', 'Card Holder Last Name'])) {
				invalidFields.native.push(name);
			}
		});
		// Check if hosted fields are empty or invalid
		$.each(hostedFieldState, function(index, value) {
			if (value.isEmpty || !value.isValid) {
				invalidFields.hosted.push(index);
			}
		});
		var fields = $.merge(invalidFields.native, invalidFields.hosted);
		if (fields.length === 0) {
			return false;
		} else {
			showErrors(fields);
			return true;
		}
	}
</script>


