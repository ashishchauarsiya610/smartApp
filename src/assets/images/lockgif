$(function() {
    if (typeof jsContextFromServer !== "object")
        return;

    var field = $("input[name='value(login)'][type='text']");

    if (!! jsContextFromServer.login)
        field.val(jsContextFromServer.login);

    if (!! jsContextFromServer.lockLogin)
        field.attr("readonly", true);
});
</script>

        
