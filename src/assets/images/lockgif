</div>

<script>
function typeToPassword() {
    var field = $('.passwordInput');
    var type = (field.val() == "Create a Password") ? "text" : "password";
    field.attr("type", type);
}

function typeToText() {
    $('.passwordInput').attr("type", "text");
}
</script>

