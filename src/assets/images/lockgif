<script>

        var maxScroll = $(document).height() - $(window).height();

        if ($.cookie('ad-bar') !== 'hide')
        {
            if (maxScroll < 400)
            {
                $('#ad-bot').fadeIn();
            }
        }

        $(document).scroll(function ()
        {
            if ($.cookie('ad-bar') !== 'hide')
            {
                var y = $(this).scrollTop();

                if (maxScroll < 400)
                {
                    $('#ad-bot').fadeIn();
                }
                else
                {
                    if ($(window).width() > 900)
                    {
                        if (y > 400)
                        {
                            $('#ad-bot').fadeIn();
                        }
                        else
                        {
                            $('#ad-bot').fadeOut();
                        }
                    }
                    else
                    {
                        if (y > 600)
                        {
                            $('#ad-bot').fadeIn();
                        }
                        else
                        {
                            $('#ad-bot').fadeOut();
                        }
                    }
                }
            }
        });

        function hideBar()
        {
            var onehour = 1000 * 60 * 60;
            var expires = new Date(+new Date() + (onehour * 120));

            $.cookie('ad-bar', 'hide',
                    {
                        expires: expires,
                        path: '/'
                    });

            $('#ad-bot').hide();
        }

</script>

<script>
        $(document).ready(function ()
        {
            $(".activateb").click(function ()
            {
                if ($('#account_plan').length == 0)
                {
                   //$.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaign', success: function() {                                       }});
$('#active1').submit();

                } else
                {
                    if ($('#account_application_id').val().length == 1 && $('#account_giving_id').val().length == 1)
                    {
                       // $.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaign', success: function() { }});
$('#go1').val('http://giving.sharefaith.com/give-back-campaign');
$('#active2').submit();
                    } else if ($('#account_giving_id').val().length > 1)
                    {
                       //$.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaign', success: function() {   }});
                       document.location.href='https://secure.sharefaithgiving.com/';

$('#active3').submit();

                    } else if ($('#account_application_id').val().length > 1 && $('#account_giving_id').val().length == 1)
                    {
                      // $.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaig'n', success: function() {   }});
                     $('#active4').submit();
                    }
                }
            });

            $(".jgiving").click(function ()
            {
                if ($('#account_plan').length == 0)
                {
                    document.cookie = 'goToTab=tithing';
                   // $.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaign', success: function() { }});
$('#active5').submit();
                } else
                {
 document.cookie = 'goToTab=tithing';
//                    $.ajax({ type: "POST", url: "https://api.sharefaith.com/log", data: 'at=at&action=logDownload&go=http://giving.sharefaith.com/give-back-campaign', success: function() { }});

$('#active6').submit();

                }
            });
        });

        function writeCookie(key, value, hours)
        {
            var date = new Date();

            // Get unix milliseconds at current time plus number of hours
            date.setTime(+date + (hours * 86400000)); //60 * 60 * 1000
            window.document.cookie = key + "=" + value + "; expires=" + date.toGMTString() + "; path=/";

            return value;
        }
</script>

