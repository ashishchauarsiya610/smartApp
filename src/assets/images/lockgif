<!-- The bar for the gdpr-->
<div id="gdpr-bot">
	<!-- Describes the with of the content container -->
  <div class="head-gdpr-con">
			<!-- The container to put the content relative in the center -->
  		<div class="head-gdpr">
					<!-- The content of the gdpr bar -->
        	<div id="gdpr-content">
							<p id="gdpr-text" >
We use cookies to ensure that we give you the best experience possible. If you would like more information on how cookies are used, please continue to more info.  Or you can click Okay to proceed as accepted.
							<div class="gdprClose" onclick="hideGDPRBar();">Okay</div>

            	<div class='gdprInfo' onclick="document.location.href='//sharefaith.com/faith/privacyPolicy/view.do';">More Info</div>

        	</div>
					<!-- Closes gdpr-content -->
    	</div>
			<!-- Closes head-gdpr -->
	</div>
	<!-- Closes head-gdpr-con -->
</div>
<!-- Closes gdpr-bot -->

<script>
	/* Only show gdpr if there is no cookie set */
  if ($.cookie('gdpr-bar') !== 'hide')
  {
		//Show the cookie
    $('#gdpr-bot').fadeIn();

		//move the ad bar up so we can see it.
		$('#ad-bot').css("bottom","70px");
		$(".ad_banner_mobile_styles").text("@media (max-width: 860px) {#ad-bot{bottom:115px !important;}} @media (max-width: 560px) {#ad-bot{bottom:138px !important;}}");
  }

	/* Set Cookie so we do not show bar again */
  function hideGDPRBar()
  {
		//Get an hour in miliseconds
    var onehour = 1000 * 60 * 60;
		//Set the expire dat to one month
    var expires = new Date(+new Date() + (onehour * 120));

		//Create the cookie
    $.cookie('gdpr-bar', 'hide',
      {
          expires: expires,
          path: '/'
      });

		//Hide the gdpr bar.
    $('#gdpr-bot').hide();

		//Put the ad bar back in place.
		$('#ad-bot').css("bottom","0");
		$(".ad_banner_mobile_styles").text("@media (min-width: 860px) {#ad-bot{bottom:0px}} @media (min-width: 560px) {#ad-bot{bottom:0px}}");
  }
</script>

        </div>
    </div>
</footer>
